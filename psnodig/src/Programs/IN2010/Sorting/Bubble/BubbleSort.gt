func BubbleSort(A int*) {
    # n := length(A)
    for i := 0, n - 2 {
        for j := 0, n - i - 2 {
            if A[j] > A[j+1] {
                @{swap A[j] with A[j+1]}{
                    tmp := A[j]
                    A[j+1] := A[j]
                    A[j] := tmp
                }
            }
        }
    }
    return A
}

BubbleSort([])