func BucketSort(A int*, N int) {
    @{B := Array of N empty lists}{
        B := array(N)
        for i := 1, N {
            append(B, list())
        }
    }

    # n := length(A)
    for i := 0, n - 1 {
        @{associate k with A[i]}{ k := A[i] % N }
        append(B[k], A[i])       
    }

    j := 0

    for k := 0, N - 1 {
        for x := B[k] {
            A[j] := x
            j := j + 1
        }
    }

    return A
}

BucketSort([64, 25, 12, 22, 11, 90, 77, 55, 44])