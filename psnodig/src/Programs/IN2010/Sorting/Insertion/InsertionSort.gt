func InsertionSort(A int*) {
    # n := length(A)
    for i := 1, n - 1 {
        j := i
        while (j > 0) && (A[j-1] > A[j]) {
            @{swap(A[j], A[j-1])}{
                tmp := A[j]
                A[j] := A[j-1]
                A[j-1] := tmp
            }
            j := j - 1
        }
    }
}

InsertionSort([64, 34, 25, 12, 22, 11, 90])